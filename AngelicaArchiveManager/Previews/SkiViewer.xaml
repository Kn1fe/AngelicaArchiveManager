<Window x:Class="AngelicaArchiveManager.Previews.SkiViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AngelicaArchiveManager.Previews"
        xmlns:hvd="http://helix-toolkit.org/wpf"
        mc:Ignorable="d"
        Title="SkiViewer" Height="450" Width="800">
    <Grid>
        <hvd:HelixViewport3D Name="Viewport" ShowFrameRate="True" ShowCoordinateSystem="True" CameraRotationMode="Trackball">
            <hvd:SunLight />
            <ModelVisual3D x:Name="Model" />
        </hvd:HelixViewport3D>
    </Grid>
</Window>
